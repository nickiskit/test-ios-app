{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": ["> 0.25%", "not dead", "iOS >= 9"],
        "useBuiltIns": "entry",
        "corejs": "3.26"
      }
    ],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-transform-runtime"
  ]
}
